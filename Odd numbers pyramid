
// you can make a odd numbers triangle using this simple code :)

class HelloWorld {
    public static void main(String[] args) {
        
        int n=5;
        
        for(int i=1;i<=n;i++){
            System.out.println();
            
            for(int j=1;j<=2*i-1;j++){
                System.out.print(j);
            }
            
        }
        
    }
}

/* output :- 
1
123
12345
1234567
123456789
=== Code Execution Successful === */
